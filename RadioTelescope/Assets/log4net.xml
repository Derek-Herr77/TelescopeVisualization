<log4net>
	<appender name="ConsoleLogger" type="ConsoleLogger">
		<layout type="log4net.Layout.PatternLayout, log4net">
			<param name="ConversionPattern" value="%d{ABSOLUTE} %-5p %c{1}:%L - %m%n"/>
		</layout>
	</appender>
	<root>
		<level value="DEBUG"/>
		<appender-ref ref="ConsoleLogger"/>
	</root>
</log4net>